{
  "name": "@alekstar79/utility",
  "version": "1.0.2",
  "description": "Utilities TS",
  "keywords": [
    "utility",
    "helpers",
    "function"
  ],
  "license": "MIT",
  "author": "Aleksey Tarasenko <alekstar79@yandex.ru>",
  "homepage": "https://github.com/alekstar79/utility.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alekstar79/utility.git"
  },
  "bugs": {
    "url": "https://github.com/alekstar79/utility/issues"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "sideEffects": false,
  "module": "./dist/utils.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/utils.es.js"
    },
    "./common/*": "./dist/common/*.js",
    "./animation/*": "./dist/animation/*.js",
    "./observers/*": "./dist/observers/*.js",
    "./dom/*": "./dist/dom/*.js",
    "./prng/*": "./dist/prng/*.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.config.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:clear": "jest --clearCache",
    "test:ci": "jest --ci --coverage",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "prepublishOnly": "yarn build",
    "transform": "tsx scripts/transform.ts"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@jest/types": "^29.6.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "canvas": "^3.2.1",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
